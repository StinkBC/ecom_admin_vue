    "element-ui": "^2.13.0",
    "fastclick": "^1.0.6",
    "koa": "^2.5.3",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "path": "^0.12.7",
    "vue2-ace-editor": "^0.0.11",
    "clipboard": "^2.0.1",
    "postcss-plugin-px2rem": "^0.8.1",
    "vue": "2.5.17",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "winston":"^3.2.1",
    "xlsx":"^0.15.5",
    "mint-ui":"^2.2.13",
   "vue2-editor":"^2.10.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^3.0.3",
    "concurrently": "^5.1.0",
    "cross-env": "^5.2.0",
